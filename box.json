{
    "algorithm": "OPENSSL",    
    "chmod": "0755",
    "compression": "GZ",
    "directories": [
        "src",
        "config"
    ],
    "files": [
        "autoload.php"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["Tests"],
            "in": "vendor"
        }
   ],
   "git-version": "package_version",
   "intercept": true,
   "main": "bin/toolrepo",
   "output": "build/toolrepo.phar",
   "stub": true,
   "key": "private.key"
}
